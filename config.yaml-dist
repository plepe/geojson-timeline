map:
  location: [ 0.0, 0.0 ]
  zoom: 2

source:
  title: Countries
  attribution: Natural Earth
  url: https://datahub.io/core/geo-countries/r/countries.geojson

feature:
  type: start-end-field
  startField: |-
    {{ item.properties.start }}
  endField: |-
    {{ item.properties.end }}
  popupTemplate: |-
    {{ item.properties.popup }}
  nodeFeature: marker
  popupTemplate: |-
    {{ item.properties.ADMIN }}
  styleTemplate: |-
    {{ { color: 'red' }|json_encode }}
